numbers:
[+-]?((0|[1-9][0-9]*)([.][0-9]*([eE][+-]?[0-9]+)?)?)|(?3)

integers:
[+-]?(0|[1-9][0-9]*)

floats:
[+-]?([0-9]+[.][0-9]*|[.][0-9]+)([eE][+-]?[0-9]+)?

strings:
"[\x9\x20\x21\x23-\x7E\p{Greek}]*"|'[\x9\x20-\x26\x28-\x7E\p{Greek}]*'

variables:
[_a-zA-Z\p{Greek}][_0-9a-zA-Z\p{Greek}]*

operators:
\=|([+\-\%]|\*{1,2}|\/{1,2})\=?

comments:
#[\x20-\x7E\p{Greek}]*|\"{3}[\xA\x20-\x7E\p{Greek}]*\"{3}
